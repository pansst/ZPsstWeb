		<property name="salePrice1" column="sale_price1"/>
		<property name="saleNum2" column="sale_num2"/>
		<property name="salePrice2" column="sale_price2"/>
		<property name="saleNum3" column="sale_num3"/>
		<property name="salePrice3" column="sale_price3"/>
		<property name="saleNum4" column="sale_num4"/>
		<property name="salePrice4" column="sale_price4"/>
		<property name="saleNum5" column="sale_num5"/>
		<property name="salePrice5" column="sale_price5"/>
		<property name="buyNum1" column="buy_num1"/>
		<property name="buyPrice1" column="buy_price1"/>
		<property name="buyNum2" column="buy_num2"/>
		<property name="buyPrice2" column="buy_price2"/>
		<property name="buyNum3" column="buy_num3"/>
		<property name="buyPrice3" column="buy_price3"/>
		<property name="buyNum4" column="buy_num4"/>
		<property name="buyPrice4" column="buy_price4"/>
		<property name="buyNum5" column="buy_num5"/>
		<property name="buyPrice5" column="buy_price5"/>
		<property name="createTime" column="create_time"/>
    </class>
</hibernate-mapping>
