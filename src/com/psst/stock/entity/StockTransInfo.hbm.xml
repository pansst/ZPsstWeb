		<property name="realPrice" column="real_price"/>
		<property name="maxPrice" column="max_price"/>
		<property name="minPrice" column="min_price"/>
		<property name="dealStocks" column="deal_stocks"/>
		<property name="dealMoney" column="deal_money"/>
